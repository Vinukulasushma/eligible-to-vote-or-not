#include<iostream>
#include<conio.h>
using namespace std;
main()
{
	int age;
	cout<<"enter your age";
	cin>>age;
	if(age>=18)
	{
		cout<<"you are eligible for voting"<<endl;
	}
	else if(age>0 && age<18)
	{
		cout<<"you are not eligible for voting"<<endl;
	}
	else
	{
		cout<<"invalid age"<<endl;
	}
	getch();
}
